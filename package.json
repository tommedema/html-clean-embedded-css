{
  "name": "html-clean-embedded-css",
  "version": "1.1.0",
  "description": "Expects html, removes redundant css within style tags, and returns html.",
  "homepage": "https://github.com/tommedema/html-clean-embedded-css",
  "bugs": {
    "url": "https://github.com/tommedema/html-clean-embedded-css/issues",
    "email": "tommedema@gmail.com"
  },
  "repository": "git@github.com:tommedema/html-clean-embedded-css.git",
  "main": "src/index.js",
  "scripts": {
    "test": "./node_modules/standard/bin/cmd.js && ./node_modules/mocha/bin/mocha",
    "standard": "./node_modules/standard/bin/cmd.js",
    "standard fix": "./node_modules/standard/bin/cmd.js --fix",
    "docs": "./node_modules/documentation/bin/documentation.js lint src/** && ./node_modules/documentation/bin/documentation.js build src/** -f md >> README.md"
