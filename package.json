  "author": {
    "name": "Tom Medema",
    "email": "tommedema@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "documentation": "^5.3.2",
    "mocha": "^3.5.3",
    "should": "^13.1.0",
    "standard": "^10.0.3"
  },
  "dependencies": {
    "css-razor": "^2.4.3",
    "i-replace": "^1.0.2",
    "styles-from-html": "^1.0.1"
  }
}
